   "source": [
    "import numpy as np\n",
    "\n",
    "arr = np.array([1, 2, 3, 2, 3, 4, 1])\n",
    "\n",
    "# Check if element has already appeared before (i.e., is a duplicate)\n",
    "duplicates = np.zeros_like(arr, dtype=bool)\n",
    "\n",
    "for i in range(1, len(arr)):\n",
    "    duplicates[i] = arr[i] in arr[:i]\n",
    "\n",
    "print(duplicates)"
   ]
